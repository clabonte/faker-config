{
    "name": "clabonte/faker-config",
    "type": "library",
    "description": "Faker extension to populate entities via a simple JSON configuration file",
    "keywords": ["faker", "fixtures", "data"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Labonte"
        }
    ],
    "require": {
        "fzaninotto/faker": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "FakerConfig\\": "src/FakerConfig"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FakerConfig\\": "tests/FakerConfig"
        }
    }
}
